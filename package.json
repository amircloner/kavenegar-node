{
  "name": "kavenegar",
  "version": "1.2.1",
  "description": "Kavenegar Node.js SDK (SMS, Voice, Verify Lookup) with TypeScript typings â€“ supports tag, hide, localid, scheduling",
  "main": "index.js",
  "types": "dist/kavenegar.d.ts",
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.js",
      "types": "./dist/kavenegar.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "postinstall": "npm run build",
    "prepublishOnly": "npm run build",
    "test": "jest --runInBand",
    "test:unit": "jest --selectProjects unit --runInBand",
    "test:integration": "jest --selectProjects integration --runInBand",
    "test:functional": "jest --selectProjects functional --runInBand",
    "coverage": "jest --coverage --runInBand",
    "package": "npm run build && npm run package:create",
    "package:create": "node scripts/create-package.js",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KaveNegar/kavenegar-node.git"
  },
  "keywords": [
    "SMS",
    "kavenegar",
    "SDK",
    "verify",
    "lookup",
    "otp",
    "voice",
    "tag",
    "hide",
    "localid",
    "schedule",
    "typescript"
  ],
  "author": "Arya sadeghy <Aryasadeghy@gmail.com> (http://yourblog.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KaveNegar/kavenegar-node/issues"
  },
  "sideEffects": false,
  "dependencies": {
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.11",
    "nock": "^13.5.4"
  },
  "homepage": "https://github.com/KaveNegar/kavenegar-node#readme"
}
